extend layout

block content
  div(class="laptoplist container")

    table(class="table")
      thead(class="thead-dark")
        tr
          th(scope="col") Model
          th(scope="col") Stock
      tbody
        each laptop in laptopList
          tr
            td
              a(href=laptop.url) #{laptop.name}
            td #{laptop.number_in_stock}
    
    a(href='/laptop/create') Create new laptop