extend layout

block content
  div(class="card" style="width: 18rem;")
    div(class="card-body")
      h5(class="card-title")= type.name
      p(class="card-text") #{type.description}
      p Laptops with this type
      ul(class="list-group list-group-flush")
        each laptop in typeLaptops
          li(class="list-group-item") 
            a(href=laptop.url) #{laptop.name}